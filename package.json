  "license": "MIT",
  "keywords": [
    "project",
    "readme",
    "license",
    "meta",
    "build",
    "buildtool",
    "metabuild",
    "generator",
    "markdown",
    "documentation",
    "package.json",
    "jquery.json",
    "component",
    "bower",
    "jspm",
    "browserify",
    "badges",
    "component.json",
    "bower.json"
  ],
  "badges": {
    "list": [
      "travisci",
      "npmversion",
      "npmdownloads",
      "daviddm",
      "daviddmdev",
      "---",
      "slackin",
